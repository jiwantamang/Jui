{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "jui-date-converter": [
        "dist/jui-date-converter/jui-date-converter",
        "dist/jui-date-converter"
      ],
      "jui-datatable": [
        "dist/jui-datatable/jui-datatable",
        "dist/jui-datatable"
      ],
      "jui-autocomplete": [
        "dist/jui-autocomplete/jui-autocomplete",
        "dist/jui-autocomplete"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}